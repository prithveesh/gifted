{
  "name": "search-my-gify",
  "version": "1.0.0",
  "author": "Prithveesh",
  "license": "MIT",
  "scripts": {
    "dev": "node ./server/index.js",
    "build": "next build --config ./config/next/next.config.js",
    "start": "next start --config ./config/next/next.config.js",
    "webpackDev:server": "npm run build:server:once && npm-run-all --parallel nodemon:prod watch:server",
    "build:server:once": "cross-env NODE_ENV=development webpack --config ./config/webpack.server.config.js",
    "watch:server": "cross-env NODE_ENV=development webpack --config ./config/webpack.server.config.js --watch",
    "nodemon:prod": "cross-env NODE_ENV=development node-dev ./build/server/server.prod.js"
  },
  "dependencies": {
    "@babel/core": "^7.4.0",
    "@babel/polyfill": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "@babel/preset-react": "^7.0.0",
    "@zeit/next-sass": "^1.0.1",
    "babel-loader": "^8.0.5",
    "clean-webpack-plugin": "^2.0.1",
    "cross-env": "^5.2.0",
    "eslint": "^5.16.0",
    "next": "^8.0.3",
    "node-dev": "^3.1.3",
    "node-sass": "^4.11.0",
    "npm-run-all": "^4.1.5",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-log": "^2.0.0",
    "webpack-node-externals": "^1.7.2"
  }
}
